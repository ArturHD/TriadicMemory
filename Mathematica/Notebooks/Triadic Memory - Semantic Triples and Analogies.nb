         RowBox[{"or", " ", "z"}]}], " ", "*)"}], "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"f", "[", 
         RowBox[{
          RowBox[{"Verbatim", "[", "_", "]"}], ",", " ", "y_SparseArray", ",",
           " ", "z_SparseArray"}], "]"}], " ", ":=", " ", 
        RowBox[{"f", "[", " ", 
         RowBox[{"Plus", " ", "@@", " ", 
          RowBox[{"(", " ", 
