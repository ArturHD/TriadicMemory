     ":=", " ", 
    RowBox[{"Module", "[", "  ", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"W", ",", " ", "tup"}], "}"}], ",", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
       "random", " ", "sparse", " ", "vector", " ", "with", " ", "dimension", 
        " ", "n", " ", "and", " ", "sparse", " ", "population", " ", "p"}], 
       " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"f", "[", "]"}], " ", ":=", " ", 
        RowBox[{"SparseArray", "[", "  ", 
         RowBox[{
          RowBox[{
           RowBox[{"RandomSample", "[", " ", 
            RowBox[{
             RowBox[{"Range", "[", "n", "]"}], ",", " ", "p"}], "]"}], 
           "\[Rule]", " ", 
           RowBox[{"Table", "[", 
            RowBox[{"1", ",", " ", "p"}], "]"}]}], ",", " ", 
          RowBox[{"{", "n", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
       "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{"initialize", " ", "memory"}], " ", "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{"W", "=", " ", 
        RowBox[{"Table", "[", " ", 
         RowBox[{"0", ",", " ", "n", ",", "n", ",", "n"}], "]"}]}], ";", 
       "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{
        "memory", " ", "addresses", " ", "activated", " ", "by", " ", "input",
          " ", "vectors"}], " ", "*)"}], "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"tup", "[", "x__SparseArray", "]"}], " ", ":=", " ", 
        RowBox[{"Tuples", "[", " ", 
         RowBox[{
          RowBox[{
           RowBox[{"Flatten", "[", " ", 
            RowBox[{"#", "[", "\"\<NonzeroPositions\>\"", "]"}], " ", "]"}], 
           "&"}], " ", "/@", " ", 
          RowBox[{"{", "x", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
       "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{
         RowBox[{"binarize", " ", "a", " ", "vector"}], ",", " ", 
         RowBox[{"using", " ", "sparsity", " ", "target", " ", "p"}]}], " ", 
        "*)"}], "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"f", "[", "0", "]"}], " ", "=", " ", 
        RowBox[{"SparseArray", "[", 
         RowBox[{
          RowBox[{"{", "0", "}"}], ",", " ", 
          RowBox[{"{", "n", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"f", "[", " ", "x_", "]"}], " ", ":=", " ", 
        RowBox[{"Module", "[", " ", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"t", "=", " ", 
            RowBox[{"Max", "[", 
             RowBox[{"1", ",", 
              RowBox[{"RankedMax", "[", 
               RowBox[{"x", ",", " ", "p"}], "]"}]}], "]"}]}], "}"}], ",", 
          " ", 
          RowBox[{"SparseArray", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"Boole", "[", 
              RowBox[{"#", "\[GreaterEqual]", " ", "t"}], "]"}], "&"}], " ", "/@",
             " ", "x"}], "]"}]}], "]"}]}], ";", " ", "\[IndentingNewLine]", 
       "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{"store", " ", 
         RowBox[{"{", 
          RowBox[{"x", ",", "y", ",", "z"}], "}"}]}], " ", "*)"}], 
