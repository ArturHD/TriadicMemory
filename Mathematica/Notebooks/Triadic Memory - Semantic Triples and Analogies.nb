          RowBox[{"n", ",", "p"}], "}"}]}], "]"}], ";", "\[IndentingNewLine]",
        "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"pos", "[", "x_SparseArray", "]"}], " ", ":=", " ", 
        RowBox[{"Sort", "[", 
         RowBox[{"Flatten", "[", 
          RowBox[{"x", "[", "\"\<NonzeroPositions\>\"", "]"}], "]"}], "]"}]}],
        ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"encode", "[", "x_SparseArray", "]"}], " ", ":=", " ", "x"}], 
       ";", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"encode", "[", 
         RowBox[{"Verbatim", "[", "_", "]"}], "]"}], " ", "=", " ", "_"}], 
       ";", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"encode", "[", "Null", "]"}], " ", "=", " ", 
        RowBox[{"SparseArray", "[", 
         RowBox[{
          RowBox[{"{", "0", "}"}], ",", 
          RowBox[{"{", "n", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"encode", "[", "s_", "]"}], " ", ":=", " ", 
        RowBox[{
         RowBox[{"encode", "[", "s", "]"}], " ", "=", " ", 
         RowBox[{"Module", "[", " ", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"r", " ", "=", " ", 
             RowBox[{"T", "[", "]"}]}], "}"}], ",", "  ", 
           RowBox[{
            RowBox[{
             RowBox[{"decode", "[", " ", 
              RowBox[{"pos", "[", "r", "]"}], "]"}], " ", "=", " ", "s"}], 
            ";", " ", "r"}]}], "]"}]}]}], ";", "\[IndentingNewLine]", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"decode", "[", 
         RowBox[{"SparseArray", "[", 
          RowBox[{
           RowBox[{"{", "0", "}"}], ",", 
           RowBox[{"{", "n", "}"}]}], "]"}], "]"}], " ", "=", " ", "Null"}], 
       ";", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"decode", "[", "x_SparseArray", "]"}], " ", ":=", " ", 
        RowBox[{"Module", "[", " ", 
         RowBox[{
          RowBox[{"{", "r", "}"}], ",", " ", 
          RowBox[{
           RowBox[{"r", " ", "=", " ", 
            RowBox[{"decode", "[", " ", 
             RowBox[{"pos", "[", "x", "]"}], "]"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"If", "[", " ", 
            RowBox[{
             RowBox[{
              RowBox[{"Head", "[", "r", "]"}], " ", "===", "decode"}], ",", 
             " ", "x", ",", "r"}], "]"}]}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"decode", "[", "Null", "]"}], " ", "=", " ", "Null"}], ";", 
       "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"f", "[", "]"}], " ", "=", " ", 
        RowBox[{"T", "[", "]"}]}], ";", " ", 
       RowBox[{"(*", " ", 
        RowBox[{"random", " ", "generator"}], " ", "*)"}], 
       "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"f", "[", 
         RowBox[{"x_", ",", "y_", ",", "z_"}], "]"}], " ", ":=", " ", 
        RowBox[{"decode", "[", " ", 
         RowBox[{"T", "[", " ", 
          RowBox[{
           RowBox[{"encode", "[", "x", "]"}], ",", " ", 
           RowBox[{"encode", "[", "y", "]"}], ",", " ", 
           RowBox[{"encode", "[", "z", "]"}]}], "]"}], "]"}]}], ";"}]}], 
     "\[IndentingNewLine]", "\[IndentingNewLine]", "]"}]}], ";"}], 
  "\[IndentingNewLine]"}]], "Input",
 CellLabel->"In[2]:=",ExpressionUUID->"37a7f9f3-09d1-4247-9331-8367d48d8283"]
