  RowBox[{"Module", "[", " ", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"w", ",", " ", "d"}], "}"}], ",", " ", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"w", " ", "=", " ", 
      RowBox[{"Ceiling", "[", 
       RowBox[{"Sqrt", "[", 
        RowBox[{"Length", "[", "x", "]"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"d", " ", "=", " ", 
      RowBox[{
       RowBox[{"Partition", "[", 
        RowBox[{
         RowBox[{"PadRight", "[", 
          RowBox[{
           RowBox[{"Normal", "[", "x", "]"}], ",", " ", 
           RowBox[{"w", "^", "2"}]}], "]"}], ",", " ", 
         RowBox[{"{", "w", "}"}]}], "]"}], " ", "/.", " ", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"1", "\[Rule]", " ", 
          RowBox[{"{", 
           RowBox[{"0.04", ",", "0.18", ",", "0.42"}], "}"}]}], ",", " ", 
         RowBox[{"0", "\[Rule]", " ", 
          RowBox[{"{", 
           RowBox[{"0.79", ",", "0.86", ",", "1.0"}], "}"}]}]}], "}"}]}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"Image", "[", 
      RowBox[{"d", ",", " ", 
       RowBox[{"ImageSize", "\[Rule]", " ", 
        RowBox[{"2", "*", "w"}]}]}], "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellGroupingRules->{"GroupTogetherGrouping", 10001.},
