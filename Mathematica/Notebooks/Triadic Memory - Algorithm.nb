           RowBox[{
            RowBox[{"++", 
             RowBox[{"W", "[", 
              RowBox[{"[", "##", "]"}], "]"}]}], "&"}], "  ", "@@@", " ", 
           RowBox[{"tup", "[", 
            RowBox[{"x", ",", "y", ",", "z"}], "]"}]}], ";"}], ")"}]}], ";", 
       "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{
         RowBox[{"recall", " ", "x"}], ",", " ", "y", ",", " ", 
         RowBox[{"or", " ", "z"}]}], " ", "*)"}], "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"f", "[", 
         RowBox[{
          RowBox[{"Verbatim", "[", "_", "]"}], ",", " ", "y_SparseArray", ",",
           " ", "z_SparseArray"}], "]"}], " ", ":=", " ", 
        RowBox[{"f", "[", " ", 
         RowBox[{"Plus", " ", "@@", " ", 
          RowBox[{"(", " ", 
           RowBox[{
            RowBox[{
             RowBox[{"W", "[", 
              RowBox[{"[", 
               RowBox[{"All", ",", " ", "#1", ",", "#2"}], "]"}], "]"}], " ", 
