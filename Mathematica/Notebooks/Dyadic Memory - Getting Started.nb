Cell["Adding salt or pepper noise to an SDR.", "Text",ExpressionUUID->"9bdca904-c1ec-44df-b6f5-a249c062b9bc"],

Cell[BoxData[
 RowBox[{
  RowBox[{"SDRNoise", "[", 
   RowBox[{"x_SparseArray", ",", " ", "bits_Integer"}], "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", " ", 
   RowBox[{
    RowBox[{"{", "p", "}"}], ",", "\[IndentingNewLine]", 
    "\[IndentingNewLine]", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"bits", " ", "\[GreaterEqual]", " ", "0"}], ",", " ", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{"salt", " ", "noise"}], ",", " ", 
        RowBox[{"adding", " ", "bits"}]}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"p", " ", "=", " ", 
        RowBox[{"Union", "[", 
         RowBox[{
          RowBox[{"Flatten", "[", 
           RowBox[{"x", "[", "\"\<NonzeroPositions\>\"", "]"}], "]"}], ",", 
          "  ", 
          RowBox[{"Table", "[", "  ", 
           RowBox[{
            RowBox[{"RandomInteger", "[", 
             RowBox[{"{", 
              RowBox[{"1", ",", 
               RowBox[{"Length", "[", "x", "]"}]}], "}"}], "]"}], ",", " ", 
            "bits"}], "]"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"SparseArray", "[", " ", 
        RowBox[{
         RowBox[{"p", " ", "\[Rule]", " ", 
          RowBox[{"Table", "[", 
           RowBox[{"1", ",", " ", 
            RowBox[{"Length", "[", "p", "]"}]}], "]"}]}], ",", " ", 
         RowBox[{"{", 
          RowBox[{"Length", "[", "x", "]"}], "}"}]}], " ", "]"}]}], ",", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{"pepper", " ", "noise"}], ",", " ", 
        RowBox[{"removing", " ", "bits"}]}], " ", "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"p", " ", "=", " ", 
        RowBox[{"Most", "[", 
         RowBox[{"ArrayRules", "[", "x", "]"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"SparseArray", "[", " ", 
        RowBox[{
         RowBox[{"RandomSample", "[", " ", 
          RowBox[{"p", ",", "  ", 
           RowBox[{
            RowBox[{"Length", "[", "p", "]"}], " ", "+", " ", "bits"}]}], 
          "]"}], ",", " ", 
         RowBox[{"Length", "[", "x", "]"}]}], "]"}]}]}], 
     "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", "]"}]}]], "Input",\

 CellLabel->"In[2]:=",ExpressionUUID->"9f5d9a6d-c75b-42fc-9117-2994ae6ab2b6"]
